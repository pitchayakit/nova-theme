{
    "name": "lexicon/nova-theme",
    "description": "A Laravel Nova theme.",
    "keywords": [
        "laravel",
        "nova",
        "lexicon",
        "nova theme"
    ],
    "homepage": "https://github.com/pitchayakit/nova-theme",
    "authors": [
        {
            "name": "Pitchayakit Pahamak",
            "email": "pitchayakit.p@gmail.com",
            "role": "software engineer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Lexicon\\NovaTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lexicon\\NovaTheme\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
